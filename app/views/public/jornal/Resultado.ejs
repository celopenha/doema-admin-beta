<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- Font Awesome -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" rel="stylesheet" />
  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
  <!-- MDB -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.6.0/mdb.min.css" rel="stylesheet" />

    <!-- Datatable -->
    <link href="/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">


  <title>Document</title>
</head>
<header>
  <!-- Intro settings -->
  <style>
    /* Default height for small devices */
    #intro-example {
      height: 800px;
    }

    /* Height for devices larger than 992px */
    @media (min-width: 992px) {
      #intro-example {
        height: 600px;
      }
    }

    @font-face {
      font-family: "Corpid";
      src: url("./vendors/fonts/corpid-black.otf");
    }

    .doema-titulo-h1 {
      font-family: "Corpid";
      font-size: 3.5em;
    }
  </style>

  <!-- Navbar -->
  <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
    <!-- Container wrapper -->
    <div class="container-fluid">
      <!-- Navbar brand -->
      <a class="navbar-brand me-2" href="https://mdbgo.com/">
        <img src="img/doema-logo-white.svg" height="38" alt="" loading="lazy" style="margin-top: -1px;" />
      </a>
      <!-- Toggle button -->
      <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#navbar-doema"
        aria-controls="navbar-doema" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fas fa-bars"></i>
      </button>
      <!-- Collapsible wrapper -->
      <div class="collapse navbar-collapse" id="navbar-doema">
        <!-- Left links -->
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link text-light" href="#">Início</a>
          </li>
          <!-- Dropdown informativo -->
          <li class="nav-item dropdown">
            <a class="nav-link text-light dropdown-toggle" href="#" id="navInformativo" role="button"
              data-mdb-toggle="dropdown" aria-expanded="false">
              Informativo
            </a>
            <ul class="dropdown-menu" aria-labelledby="navInformativo">
              <li>
                <a class="dropdown-item" href="#">História</a>
              </li>
              <li>
                <a class="dropdown-item" href="#">Legislação do Diário Oficial</a>
              </li>
              <li>
                <hr class="dropdown-divider" />
              </li>
              <li>
                <a class="dropdown-item" href="#">Something else here</a>
              </li>
            </ul>
          </li>
          <!-- Dropdown publicacao -->
          <li class="nav-item dropdown">
            <a class="nav-link text-light dropdown-toggle" href="#" id="navPublicacoes" role="button"
              data-mdb-toggle="dropdown" aria-expanded="false">
              Publicações
            </a>
            <ul class="dropdown-menu" aria-labelledby="navPublicacoes">
              <li>
                <a class="dropdown-item" href="#">Modelo de Publicação</a>
              </li>
              <li>
                <a class="dropdown-item" href="#">Normas de Publicação</a>
              </li>
              <li>
                <hr class="dropdown-divider" />
              </li>
              <li>
                <a class="dropdown-item" href="#">Prazo de Publicação</a>
              </li>
            </ul>
          </li>
          <!-- Dropdown biblioteca -->
          <li class="nav-item dropdown">
            <a class="nav-link text-light dropdown-toggle" href="#" id="navBiblioteca" role="button"
              data-mdb-toggle="dropdown" aria-expanded="false">
              Biblioteca
            </a>
            <ul class="dropdown-menu" aria-labelledby="navBiblioteca">
              <li>
                <a class="dropdown-item" href="#">Acervo Local</a>
              </li>
              <li>
                <a class="dropdown-item" href="#">Pesquisar Documentos Antigos</a>
              </li>
            </ul>
          </li>
        </ul>
        <div class="d-flex align-items-center">
          <button type="button" class="btn btn-light me-3">
            Publique no diário Oficial
          </button>
          <a class="btn btn-dark px-3" href="https://github.com/mdbootstrap/mdb-ui-kit" role="button">
            CREDENCIAMENTO
          </a>
        </div>
      </div>
      <!-- Collapsible wrapper -->
    </div>
    <!-- Container wrapper -->
  </nav>
  <!-- Navbar -->

</header>

<body>

  <div class="container">

    <table class="  table-striped table-hover js-basic-example dataTable" id="example" width="100%">
      <thead>
        <tr>
          <th>fragmento</th>
          <th>Título</th>
          <th>Página</th>
        </tr>
      </thead>
  
      <tbody id="table_body">
        <% for (var i=0; i < termo.length; i++) { %>
          <tr>
            <td>
              <%-termo[i].fragmento %>
            </td>
            <td>
              <%=termo[i].titulo %>
            </td>
            <td>
              <%=termo[i].pagina %>
            </td>
          </tr>
          <% } %>
      </tbody>
      <tfoot>
        <th>fragmento</th>
        <th>Título</th>
        <th>pagina</th>
      </tfoot>
    </table>
  </div>


  <script src="/vendor/datatables/jquery.dataTables.min.js"></script>
  <script src="/vendor/datatables/dataTables.bootstrap4.min.js"></script>

  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.6.0/mdb.min.js"></script>


  <script>
    $(document).ready(function () {
      $('#example').DataTable();
    });
  </script>
  
</body>

</html>