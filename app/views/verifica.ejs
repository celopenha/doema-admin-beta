    <!-- Verifica o tipo de cadastro desejado - [ start ]-->
    <script>
        // $("#div_ende").hide();
        $("#responsavel").on('change', function () {
            $("#div_ende").show()
        });

        $("#escola").on('change', function () {
            window.location.href = '/app/escola/create/';
        });
    </script>
    <!-- Verifica o tipo de cadastro desejado - [ end ]-->

    <!-- Pega do banco lista de municípios - [ start ]-->
    <script>
        function getMunicipios() {
            var estadoId = $('#estado').val();
            $('#municipio').empty()
            $.ajax({
                type: "GET",
                url: "/app/instituicao/municipio/" + estadoId,
                success: function (data) {
                    $.each(data, function (i, d) {
                        $('#municipio').append('<option value="' + d.id + '">' + d.nome +
                            '</option>');
                    });
                }
            })
        }
    </script>
    <!-- Pega do banco lista de municípios - [ end ]-->

    <!-- Mascaras do formulário - [ start ]-->
    <script>
        $(document).ready(function () {
            $('#cep').mask('00000-000');
            $('#cpf').mask('000.000.000-00');
            var options = {
                onKeyPress: function (tel, e, field, options) {
                    var masks = ['(00) 00000-0000', '(00) 0000-0000'];
                    var mask = (tel.slice(5).indexOf("9") == 0) ? masks[0] : masks[1];
                    $('#celular').mask(mask, options);
                }
            };
            $('#celular').mask('(00) 00000-0000', options)
            $('#fone').mask('(00) 0000-0000');
            $('#cnpj').mask('00.000.000/0000-00');
            $('#horaInicio').mask('DD/MM/YYYY hh:mm');
            $('#horaFim').mask('DD/MM/YYYY hh:mm');
            $('#dia').mask('DD/MM/YYYY hh:mm');
        });
    </script>
    <!-- Mascaras do formulário - [ end ]-->

    <!-- Validação do formulário - [ start ]-->
    <script>
        controleEmail = 0

        function verificaEmail() {
            if (controleEmail == 0) {
                controleEmail++;
                return false;
            }
            var email = $('#email').val();
            var emailrepetido = $('#emailrepetido').val();
            if (emailrepetido != email) {
                alert('Emails não correspondem.');
                $('[type=submit]').prop('disabled', true);
            } else {
                $('[type=submit]').prop('disabled', false);
            }
        }

        controleSenha = 0

        function verificaSenha() {
            if (controleSenha == 0) {
                controleSenha++;
                return false;
            }
            var senha = $('#password').val();
            var senharepetida = $('#passwordrepetido').val();
            if (senharepetida != senha) {
                alert('Senhas não correspondem.')
                $('[type=submit]').prop('disabled', true);
            } else {
                $('[type=submit]').prop('disabled', false);
            }
        }
    </script>
    <!-- Validação do formulário - [ end ]-->
